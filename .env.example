# Frontend (Vite) environment variables
# Copy to .env and fill real values.

VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.europe-west1.firebasedatabase.app
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=1234567890
VITE_FIREBASE_APP_ID=1:1234567890:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Report form reCAPTCHA v3 site key (required when CAPTCHA is enabled)
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key

# Optional: custom endpoint for report submit function
# VITE_SUBMIT_REPORT_URL=https://europe-west1-your-project-id.cloudfunctions.net/submitReport
